<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="TextAppearance.AppCompat.Widget.ActionBar.Title"
        parent="@android:style/TextAppearance.DeviceDefault.Large">
        <item name="android:textSize">30sp</item>
    </style>

    <style name="actionbarStyle.Light"
           parent="@style/Widget.AppCompat.Light.ActionBar">
    </style>

    <style name="actionbarStyle"
        parent="@style/Widget.AppCompat.ActionBar">
    </style>

    <!-- Application theme. -->
    <style name="AppTheme.Dark" parent="AppBaseTheme">
    </style>

    <!-- Application theme. -->
    <style name="AppTheme.Light" parent="AppBaseTheme.Light">
    </style>

    <style name="LoginTheme.Dark" parent="AppThemeNoActionBar.Dark">
    </style>


    <style name="LoginTheme.Light" parent="AppThemeNoActionBar.Light">
    </style>

    <!-- Application theme. -->
    <style name="WizardTheme.Dark" parent="AppThemeNoActionBar.Dark">
    </style>

    <style name="WizardTheme.Light" parent="AppThemeNoActionBar.Light">
    </style>


    <style name="PreferencesTheme.Dark" parent="AppBaseTheme">
        <item name="android:preferenceCategoryStyle">@style/preferenceCategoryStyle</item>
    </style>


    <style name="PreferencesTheme.Light" parent="AppBaseTheme.Light">
        <item name="android:preferenceCategoryStyle">@style/preferenceCategoryStyle</item>
    </style>

    <style name="preferenceCategoryStyle">
    </style>

    <style name="DashboardWrapper">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:padding">0dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/theme_color</item>
    </style>

    <style name="DashboardTableContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
    </style>

    <style name="alertDialogTheme" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:windowTitleStyle">@style/alertTitleTextStyle</item>
        <item name="android:background">?attr/drawerBackgroundColor</item>
        <item name="android:textColor">?attr/primaryTextColor</item>
        <item name="colorAccent">@color/primary_blue</item>
    </style>

    <style name="alertDialogThemeLight" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowTitleStyle">@style/alertTitleTextStyle</item>
        <item name="android:background">?attr/drawerBackgroundColor</item>
        <item name="android:textColor">?attr/primaryTextColor</item>
        <item name="colorAccent">@color/primary_blue</item>
    </style>

    <style name="alertTitleTextStyle">
        <item name="android:textColor">@color/primary_blue</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Title</item>
    </style>

    <style name="WizardContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
    </style>

    <style name="setupMethodConfigurationsList">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginStart">25dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
    </style>

    <style name="SummaryHeader">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">9dp</item>
        <item name="android:textSize">35sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/primary_blue</item>
    </style>

    <style name="SelectableSummaryHeader">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">9dp</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/selectable_summary_header</item>
    </style>

    <style name="DashboardBaseText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">?attr/dashBoardBaseTextColor</item>
    </style>

    <style name="DashboardItemDescription">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/primary_black</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="DashboardTitleText" parent="DashboardBaseText">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:capitalize">words</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">40sp</item>
    </style>

    <style name="DashboardMediumTitleText" parent="DashboardTitleText">
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:paddingLeft">0dp</item>

    </style>

    <style name="DashboardListItemPrimaryText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dip</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">40sp</item>
    </style>

    <style name="DashboardListItemMediumHeadingText" parent="DashboardListItemPrimaryText">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>

    <style name="RegisterText" parent="DashboardBaseText">
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:textColor">#DDDDDD</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>

    <style name="ClickableSummaryItem">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">22.5dp</item>
        <item name="android:paddingRight">22.5dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/list_selector</item>
    </style>

    <style name="ClickableGridItem">
        <item name="android:layout_width">0dp</item>
        <item name="android:background">@drawable/grid_buttons</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginStart">7.5dp</item>
        <item name="android:layout_marginEnd">7.5dp</item>
        <item name="android:paddingLeft">7.5dp</item>
        <item name="android:paddingRight">7.5dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="NotClickableSummaryItem">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">22.5dp</item>
        <item name="android:paddingRight">22.5dp</item>
        <item name="android:textSize">35sp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="SummaryItemRow">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:paddingTop">7.5dp</item>
        <item name="android:paddingBottom">7.5dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="AutocompleteTextArea">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:paddingTop">7.5dp</item>
        <item name="android:paddingBottom">7.5dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
    </style>

    <style name="ButtonRow">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="DashboardRefreshBar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left</item>
        <item name="android:background">#222222</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="DividerLine">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/primary_blue</item>
    </style>

    <style name="DividerLineMargin">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#555555</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="DashboardTextButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dashboardButtonHeight</item>
        <item name="android:layout_weight">.5</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:padding">7.5dp</item>
        <item name="android:background">#555555</item>
        <item name="android:textColor">#eeeeee</item>
        <item name="android:drawablePadding">6dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ListHeader">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">7.5dp</item>
        <item name="android:textColor">?attr/primaryTextColor</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="ListDescription">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">7.5dp</item>
        <item name="android:textColor">?attr/secondaryTextColor</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="ListSelectableDescription">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">7.5dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/list_multi_selector_blue</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
    </style>

    <style name="PatientDemographicsLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">22.5dp</item>
        <item name="android:paddingRight">22.5dp</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
    </style>

    <style name="ListSectionHeader">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:background">@color/list_section_header_background</item>
    </style>

    <style name="ListSectionHeaderLeftText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ListSectionHeaderRightText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="FormSaveDateTime">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">right</item>
        <item name="android:textColor">?attr/primaryTextColor</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="ObservationGroupByConceptHeader">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="ObservationGroupByEncounterHeader">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ObservationListElement">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ObservationHorizontalLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">3dp</item>
    </style>

    <style name="ObservationByConceptElement">
        <item name="android:layout_width">@dimen/observation_element_by_concept_height</item>
        <item name="android:layout_height">@dimen/observation_element_by_concept_height</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@android:color/white</item>
    </style>

    <style name="ObservationByEncounterElement">
        <item name="android:layout_width">@dimen/observation_element_by_encounter_height</item>
        <item name="android:layout_height">@dimen/observation_element_by_encounter_height</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@android:color/white</item>
    </style>

    <style name="ObservationValue">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">.6</item>
        <item name="android:padding">8dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="ObservationConcept">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">.5</item>
        <item name="android:padding">8dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="ObservationDate">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">.4</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">27sp</item>
    </style>
    <style name="EncounterLocation">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">.5</item>
        <item name="android:gravity">left</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">25sp</item>
    </style>
    <style name="EncounterDate">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">.4</item>
        <item name="android:gravity">right</item>
        <item name="android:textColor">?attr/primaryTextColor</item>
        <item name="android:textSize">25sp</item>
    </style>
    <style name="EncounterProvider">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">.6</item>
        <item name="android:gravity">left</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">25sp</item>
    </style>

    <style name="EncounterTypeRow">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="EncounterFormName">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">.4</item>
        <item name="android:textSize">30sp</item>
        <item name="android:gravity">left</item>
        <item name="android:textColor">?attr/primaryTextColor</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>

    <!--Login Screen-->
    <style name="LoginActivityLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">?attr/primaryBackgroundColor</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">64dp</item>
    </style>

    <style name="LoginActivityLogo">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">120dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:gravity">center</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:src">?attr/icLauncherLogo</item>
    </style>

    <style name="LoginActivityHeader">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">?attr/primaryTextColor</item>
        <item name="android:textSize">60sp</item>
        <item name="android:typeface">normal</item>
        <item name="android:text">@string/app_title</item>
    </style>

    <style name="LoginActivityDivider.Style">
        <item name="android:layout_marginTop">32dp</item>
        <item name="android:layout_marginBottom">32dp</item>
        <item name="android:background">?attr/loginDividerBackgroundColor</item>
    </style>

    <style name="LoginActivityDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:layout_marginTop">32dp</item>
        <item name="android:layout_marginBottom">32dp</item>
        <item name="android:background">?attr/loginDividerBackgroundColor</item>
    </style>

    <style name="LoginInfoLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">?attr/loginLayoutBackground</item>
    </style>

    <style name="LoginInfoLayoutDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">?attr/loginInfoDividerBackgroundColor</item>
    </style>

    <style name="LoginLandscapeLeftLayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="LoginLandscapeRightLayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">32dp</item>
        <item name="android:paddingRight">32dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="LoginInfoEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">96dp</item>
        <item name="android:textColor">@color/primary_blue</item>
        <item name="android:textColorHint">@color/hint_text_grey</item>
        <item name="android:paddingLeft">36dp</item>
        <item name="android:paddingRight">36dp</item>
        <item name="android:typeface">normal</item>
        <item name="android:cursorVisible">true</item>
        <item name="android:textCursorDrawable" tools:ignore="NewApi">@null</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="LoginButtonLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">96dp</item>
        <item name="android:layout_marginTop">32dp</item>
        <item name="android:layout_marginBottom">32dp</item>
    </style>

    <style name="LoginButton">
        <item name="android:background">@drawable/rounded_corner_loginbutton</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">36sp</item>
        <item name="android:textColor">?attr/primaryTextColor</item>
    </style>

    <style name="Button">
        <item name="android:layout_width">match_parent</item>
